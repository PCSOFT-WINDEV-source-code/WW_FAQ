      // Summary: Returns whether the current user is Administrator
      // 
      PROCEDURE IsAdmin()
      
      // PROD
      IF BrowserIPAddress() = NetIPAddress("ADMINISTRATORID") _OR_ BrowserIPAddress() = "localhost"  _OR_ BrowserIPAddress() = "::1" THEN
      	RESULT True
      ELSE
      	RESULT False
      END
